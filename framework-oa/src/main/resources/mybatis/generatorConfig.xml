		location="F:\apache-maven-3.1.0\repository\mysql\mysql-connector-java\5.1.25\mysql-connector-java-5.1.25.jar" />
	<context id="mysql">
		<!-- 去除自动生成的注释 -->
		<commentGenerator>
			<property name="suppressAllComments" value="true" />
		</commentGenerator>
		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://localhost:3306/dining" userId="root"
			password="root" />
